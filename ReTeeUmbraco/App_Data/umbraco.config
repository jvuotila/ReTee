        <Fairway id="1157" parentID="1125" level="4" creatorID="0" sortOrder="0" createDate="2016-01-21T18:15:15" updateDate="2016-01-21T18:16:44" nodeName="1. väylä" urlName="1-vaeylae" path="-1,1093,1124,1125,1157" isDoc="" nodeType="1156" creatorName="Juho Vuotila" writerName="Juho Vuotila" writerID="0" template="0" nodeTypeAlias="Fairway">
